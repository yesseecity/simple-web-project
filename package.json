    "build_win": "electron-builder --win"
  },
  "author": "Tid",
  "license": "MIT",
  "devDependencies": {
    "ejs": "^2.7.1",
    "electron": "^7.0.1",
    "electron-builder": "^22.1.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-ejs": "^4.1.2",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-watch": "^5.0.1",
    "node-sass": "^4.12.0",
    "package.json": "^2.0.1",
    "yamljs": "^0.3.0"
